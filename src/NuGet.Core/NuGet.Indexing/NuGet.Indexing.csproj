  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\NuGet.Versioning\NuGet.Versioning.csproj" />
    <ProjectReference Include="..\NuGet.Protocol.Core.Types\NuGet.Protocol.Core.Types.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Lucene.Net" Version="3.0.3" />
  </ItemGroup>

  <Import Project="..\..\..\build\common.core.targets"/>
</Project>
